{
    "name": "@tractorzoom/equipment-attributes",
    "version": "4.2.23",
    "description": "Source of truth for equipment attributes by category",
    "author": "Tractor Zoom",
    "license": "ISC",
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": "12.x"
    },
    "main": "lib/index.js",
    "scripts": {
        "build": "./node_modules/.bin/babel src --out-dir lib --copy-files",
        "commit": "git-cz",
        "prettier:all": "prettier --write './**/*.*'",
        "pretty-quick": "pretty-quick",
        "test": "jest"
    },
    "devDependencies": {
        "@babel/cli": "7.11.6",
        "@babel/core": "7.11.6",
        "@babel/preset-env": "7.11.5",
        "@commitlint/cli": "8.3.5",
        "@commitlint/config-conventional": "8.3.4",
        "@semantic-release/changelog": "5.0.1",
        "@semantic-release/git": "9.0.0",
        "@semantic-release/release-notes-generator": "9.0.1",
        "chance": "1.1.7",
        "commitizen": "4.2.1",
        "cz-conventional-changelog": "3.3.0",
        "husky": "4.3.0",
        "jest": "26.4.2",
        "prettier": "2.1.2",
        "pretty-quick": "2.0.2",
        "semantic-release": "17.1.1"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TractorZoom/equipment-attributes.git"
    },
    "homepage": "https://github.com/TractorZoom/equipment-attributes",
    "bugs": {
        "url": "https://github.com/TractorZoom/equipment-attributes/issues"
    },
    "keywords": [
        "equipment",
        "equipment-attributes",
        "farm",
        "farm-equipment",
        "make",
        "model"
    ]
